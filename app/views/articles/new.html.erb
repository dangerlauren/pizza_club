<%= form_for :article, url: articles_path do |f| %>
	<p>
		<%= f.label :author %><br>
		<%= f.text_field :author %>
	</p>

	<p>
		<%= f.label :date %><br>
		<%= f.date_field :date %>
	</p>

	<p>
		<%= f.label :headline %><br>
		<%= f.text_field :headline %>
	</p>

	<p>
		<%= f.label :text %><br>
		<%= f.text_area :text %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<% end %>